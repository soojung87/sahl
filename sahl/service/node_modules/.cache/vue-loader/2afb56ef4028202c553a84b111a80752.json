ction: action => { this.newfile(action) } },\r\n                { title: 'Open Project',   menuAction: action => { this.newfile(action) } },\r\n                { title: 'Rename Project', menuAction: action => { this.newfile(action) } },\r\n                { title: 'Delete Project',  menuAction: action => { this.newfile(action) } },\r\n                { title: 'Import Project',  menuAction: action => { this.newfile(action) } },\r\n                { title: 'Cut',             menuAction: action => { this.newfile(action) } },\r\n                { title: 'Copy',            menuAction: action => { this.newfile(action) } },\r\n                { title: 'Paste',           menuAction: action => { this.newfile(action) } },\r\n            ],            \r\n        }\r\n    },\r\n    methods: {\r\n        show (e) {\r\n            e.preventDefault()\r\n            this.showMenu = false\r\n            this.x = e.clientX\r\n            this.y = e.clientY\r\n            this.$nextTick(() => {\r\n                this.showMenu = true\r\n            })\r\n        },\r\n        newfile () {\r\n            alert('newfile')\r\n        },\r\n        openNode () {\r\n            // if (node == this.projectitems.name) {\r\n            //     alert(this.projectitems.name)\r\n            //     this.$store.commit('saveOpenProject', {openProjectIndex:this.projectitems.id})\r\n            // }\r\n        },\r\n        onClick (text) {\r\n            alert(`You clicked ${text}!`);\r\n        }\r\n    },\r\n})\r\n</script>\r\n<style scoped>\r\n.v-context ul{\r\n    background-color: #fff;\r\n    background-clip: padding-box;\r\n    border-radius: .25rem;\r\n    border: 1px solid rgba(0, 0, 0, 0.15);\r\n    box-shadow: 0 2px 2px 0 rgba(0, 0, 0, 0.14), 0 3px 1px -2px rgba(0, 0, 0, 0.2), 0 1px 5px 0 rgba(0, 0, 0, 0.12);\r\n    display: block;\r\n    margin: 0;\r\n    padding: 10px 0;\r\n    min-width: 10rem;\r\n    z-index: 1500;\r\n    position: fixed;\r\n    list-style: none;\r\n    box-sizing: bo