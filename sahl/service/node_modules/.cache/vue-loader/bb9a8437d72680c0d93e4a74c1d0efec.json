ntY\r\n            this.$nextTick(() => {\r\n                this.showMenu = true\r\n            })\r\n        },\r\n        newfile () {\r\n            alert('newfile')\r\n        },\r\n        openNode () {\r\n            // if (node == this.projectitems.name) {\r\n            //     alert(this.projectitems.name)\r\n            //     this.$store.commit('saveOpenProject', {openProjectIndex:this.projectitems.id})\r\n            // }\r\n        },\r\n        onClick (text) {\r\n            alert(`You clicked ${text}!`);\r\n        }\r\n    },\r\n})\r\n</script>\r\n\r\n<style>\r\n@import '~vue-context/dist/css/vue-context.css'\r\n</style>\r\n"]}]}