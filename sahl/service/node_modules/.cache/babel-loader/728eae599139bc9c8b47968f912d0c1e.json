,QAAA,QAAA,CAAA,IAAA,CAAA,KAAA,CAAA,MAAA,GAAA,WAAA;AACA,YAAA,CAAA,GACA,SAAA,KAAA,OAAA,GACA,QAAA,CAAA,IAAA,CAAA,WAAA,GAAA,CAAA,CAAA,OADA,GAEA,CAAA,CAAA,OAHA;AAIA;AACA;AACA;AACA;AACA;;AAEA,QAAA,EAAA,CAAA,KAAA,CAAA,KAAA,GAAA,CAAA,GAAA,IAAA;AACA;;AAEA,MAAA,YAAA,CAAA,gBAAA,CACA,WADA,EAEA,UAAA,CAAA,EAAA;AACA,YAAA,CAAA,CAAA,OAAA,GAAA,OAAA,EAAA;AACA,UAAA,EAAA,CAAA,KAAA,CAAA,UAAA,GAAA,SAAA;AACA,UAAA,QAAA,CAAA,gBAAA,CAAA,WAAA,EAAA,MAAA,EAAA,KAAA;AACA;AACA,OAPA,EAQA,KARA;AAWA,MAAA,QAAA,CAAA,gBAAA,CACA,SADA,EAEA,YAAA;AACA,QAAA,EAAA,CAAA,KAAA,CAAA,UAAA,GAAA,EAAA;AACA,QAAA,MAAA,CAAA,UAAA,CAAA,KAAA,GAAA,EAAA,CAAA,KAAA,CAAA,KAAA;AACA,QAAA,QAAA,CAAA,IAAA,CAAA,KAAA,CAAA,MAAA,GAAA,EAAA;AACA,QAAA,QAAA,CAAA,mBAAA,CAAA,WAAA,EAAA,MAAA,EAAA,KAAA;AACA,OAPA,EAQA,KARA;AAUA,KAxFA;AAyFA,IAAA,eAzFA,2BAyFA,IAzFA,EA0FA;AAEA,UAAA,IAAA,CAAA,MAAA,EACA;AACA,QAAA,QAAA,CAAA,cAAA,CAAA,gBAAA,EAAA,KAAA,CAAA,KAAA,GAAA,WAAA;AACA,aAAA,KAAA,GAAA,KAAA;AACA,OAJA,MAMA;AAEA,QAAA,QAAA,CAAA,cAAA,CAAA,gBAAA,EAAA,KAAA,CAAA,KAAA,GAAA,OAAA;AACA,aAAA,KAAA,GAAA,IAAA;AACA;AAEA;AAxGA,GAhDA;AA0JA,EAAA,KAAA,EAAA;AACA,IAAA,KADA,mBACA,CACA;AACA;AAHA;AA1JA,CAAA","sourcesContent":["\n<template>\n  <div>\n    <vue-draggable-resizable\n      class-name='mini-map'\n      :draggable=\"false\" :resizable=\"false\"\n      x=\"1400\" y=\"55\" w=\"300\" h=\"200\" z=\"100\">\n    </vue-draggable-resizable>\n    \n    <v-row>\n    <v-app-bar\n    dense\n    flat\n    class = \"green accent-1\">\n\n    <!--add folder button-->\n       <v-tooltip bottom>  <template v-slot:activator=\"{ on, attrs }\"><v-btn class = \"d-inline-flex ml-8 mr-4 green lighten-1\" small icon v-bind=\"attrs\"\n          v-on=\"on\" @click=\"navigation.shown = !navigation.shown\"><v-icon>mdi-menu</v-icon></v-btn></template><span>project menu</span></v-tooltip>\n\n               <v-dialog\n                    v-model=\"dialog[projectnum+1]\"\n                    width=\"500\"\n                    >\n             <template v-slot:activator=\"{ on:ondialog}\">\n                <v-tooltip bottom>  <template v-slot:activator=\"{on:ontooltip}\"><v-btn class = \"d-inline-flex ml-4 mr-4 green lighten-1\" small icon\n          v-on=\"{ ...ontooltip, ...ondialog }\"><v-icon>mdi-folder-plus</v-icon></v-btn></template><span>add folder</span></v-tooltip></template>\n                    <v-card>\n                        <v-card-title class=\"text-h5 green accent-1\">\n                        Name New Project:\n                        </v-card-title>\n                        <v-card-text>\n                        <v-text-field single-line dense label=\"project name\" v-model=\"dialogitems[projectnum+1]\"></v-text-field></v-card-text>\n                        <v-divider></v-divider>\n\n                        <v-card-actions>\n                        <v-spacer></v-spacer>\n                        <v-btn\n                            color=\"primary\"\n                            text\n                            @click=\"addproject()\"\n                        >\n                            Accept\n                        </v-btn>\n                        </v-card-actions>\n                    </v-card>\n                    </v-dialog>\n        <!--end of add folder button-->\n\n\n        <v-tooltip bottom>  <template v-slot:activator=\"{ on, attrs }\"><v-btn class = \"d-inline-flex ml-4 mr-4 green lighten-1\" small icon v-bind=\"attrs\"\n          v-on=\"on\" @click=\"removeproject()\"><v-icon>mdi-folder-remove-outline</v-icon></v-btn></template><span>remove current folder</span></v-tooltip>\n\n         <v-tooltip bottom>  <template v-slot:activator=\"{ on, attrs }\"><v-btn class = \"d-inline-flex ml-4 mr-4 green lighten-1\" small icon v-bind=\"attrs\"\n          v-on=\"on\"><v-icon>mdi-expand-all</v-icon></v-btn></template><span>expand all</span></v-tooltip>\n\n         <v-tooltip bottom>  <template v-slot:activator=\"{ on, attrs }\"><v-btn class = \"d-inline-flex ml-4 mr-4 green lighten-1\" small icon v-bind=\"attrs\"\n          v-on=\"on\"><v-icon>mdi-collapse-all-outline</v-icon></v-btn></template><span>collapse all</span></v-tooltip>\n\n        <v-tooltip bottom>  <template v-slot:activator=\"{ on, attrs }\"><v-btn class = \"d-inline-flex ml-4 mr-4 green lighten-1\" small icon v-bind=\"attrs\"\n          v-on=\"on\"><v-icon>mdi-undo</v-icon></v-btn></template><span>undo</span></v-tooltip>\n\n        <v-tooltip bottom>  <template v-slot:activator=\"{ on, attrs }\"><v-btn class = \"d-inline-flex ml-4 mr-4 green lighten-1\" small icon v-bind=\"attrs\"\n          v-on=\"on\"><v-icon>mdi-redo</v-icon></v-btn></template><span>redo</span></v-tooltip>\n\n        <v-tooltip bottom>  <template v-slot:activator=\"{ on, attrs }\"><v-btn class = \"d-inline-flex ml-4 mr-4 green lighten-1\" small icon v-bind=\"attrs\"\n          v-on=\"on\" id='validatebutton'><v-icon>mdi-play</v-icon></v-btn></template><span>validate</span></v-tooltip>\n    </v-app-bar>\n    </v-row>\n\n    <v-row>\n        <v-navigation-drawer ref=\"drawer\" class = \"indigo lighten-4\" :width=\"navigation.width\" v-model=\"navigation.shown\" height = 830>\n               <v-dialog\n                    v-model=\"dialog[projectnum+1]\"\n                    width=\"500\"\n                    >\n             <template v-slot:activator=\"{ on, attrs }\">\n                <v-btn block elevation=\"0\" tile color = \"cyan lighten-4\" v-bind=\"attrs\" v-on=\"on\">\n                    new project</v-btn></template>\n                    <v-card>\n                        <v-card-title class=\"text-h5 green accent-1\">\n                        Name New Project:\n                        </v-card-title>\n                        <v-card-text>\n                        <v-text-field single-line dense label=\"project name\" v-model=\"dialogitems[projectnum+1]\"></v-text-field></v-card-text>\n                        <v-divider></v-divider>\n\n                        <v-card-actions>\n                        <v-spacer></v-spacer>\n                        <v-btn\n                            color=\"primary\"\n                            text\n                            @click=\"addproject()\"\n                        >\n                            Accept\n                        </v-btn>\n                        </v-card-actions>\n                    </v-card>\n                    </v-dialog>\n\n                    <v-list class = \"text-start ml-3\" width = 120%>\n                        <v-list-group\n                            v-for=\"item in items\"\n                            :key=\"item.title\"\n                            v-model=\"item.active\"\n                            :prepend-icon=\"item.action\"\n                            no-action\n                            @click=\"validateprocess(item)\"\n                        >             <!--hmmmmm-->\n                            <template v-slot:activator>\n                            <v-card :ripple=\"false\" flat class = \"indigo lighten-5\" width = 120% @contextmenu=\"shows\">\n                                <v-list-item-title v-text=\"item.v\"></v-list-item-title>\n                            </v-card>\n                            </template>\n                            <component :is = \"item.c\" ></component>  \n                        </v-list-group>\n                    </v-list>\n                        \n\n                    <v-menu\n            v-model=\"showMenuList\"\n            :position-x=\"x\"\n            :position-y=\"y\"\n            :close-on-content-click=\"true\"\n            absolute\n            offset-y\n            >\n            <v-list dense class = \"text-start\">\n\n                <v-btn class = \"d-flex flex-column align-start\"\n                block\n                small\n                tile\n                v-for=\"(item, index) in menuitems\"\n                :key=\"index\">{{item.title}}\n                </v-btn>\n            </v-list>\n            </v-menu>\n        </v-navigation-drawer>\n        <v-content>\n          <mainview/>\n        </v-content>\n    </v-row>\n    \n    <div class=\"text-center\">\n    <v-bottom-sheet\n      v-model=\"sheet\"\n      hide-overlay\n      persistent\n      no-click-animation\n      content-class=\"bottomsheet\"\n      width=85%\n    >\n      <v-sheet\n        class=\"text-center\"\n        height=\"200px\"\n      >\n        <v-btn\n          class=\"mt-6\"\n          text\n          color=\"error\"\n          @click=\"sheet = false\"\n        >\n          close\n        </v-btn>\n        <div class=\"my-3\">\n          hello world\n        </div>\n      </v-sheet>\n    </v-bottom-sheet>\n  </div>\n\n</div>\n</template>\n\n<script>\nimport foldertree from '@/components/foldertree.vue'\nimport mainview from '@/components/mainview.vue'\n\n\nexport default({\n    components:{\n        foldertree, mainview\n    },\n    data: () => ({\n        sheet: false,\n        dialog:[],\n        dialogitems:[],\n        projectid:0,\n        projectnum: 0,\n        dontcallcomponent: true,\n        FolderTree: 'foldertree',\n\n        navigation: {\n        shown: false,\n        width:\"15%\",\n        borderSize: 3\n      },\n        \n         items: [\n        ],\n        showMenuList: false,\n        x: 0,\n        y: 0,\n        menuitems: [\n        { title: 'New Project' },\n        { title: 'Open Project' },\n        {title: 'Rename Project'},\n        {title: 'Delete Project'},\n        { title: 'Import Project' },\n        { title: 'Cut' },\n        { title: 'Copy' },\n        { title: 'Paste' },\n      ],\n        \n        timeoutId: null\n    }),\n\n    computed: {\n    direction() {\n      return this.navigation.shown === false ? \"Open\" : \"Closed\";\n    }\n    },\n    mounted() {\n    this.setBorderWidth();\n    this.setEvents();\n  },\n    methods:\n    {\n        addproject(){\n            this.dialog.push(false);\n            this.dialogitems.push(\"\");\n            this.projectnum++;\n            this.items.push({action: 'mdi-folder', c: 'foldertree', title: 'Project ' + this.projectnum, v: this.dialogitems[this.projectnum], canedit: true}); //smth like this\n        },\n        removeproject(){\n            for(var i = 0; i < this.items.length; i++)\n            {\n                if(this.items[i].active)\n                {\n                    this.items.splice(i,1);\n                    this.dialog.splice(i,1);\n                    this.dialogitems.splice(i,1);\n                    this.projectnum--;\n                }\n            }\n            document.getElementById('validatebutton').style.color= \"darkgreen\";\n            this.sheet = false;\n        },\n        shows (e) {\n            e.preventDefault()\n            this.showMenuList = false\n            this.x = e.clientX\n            this.y = e.clientY\n            this.$nextTick(() => {\n            this.showMenuList = true\n            })\n        },\n        getbool(n){\n            return this.items[n].bool;\n        },\n        getv(n){\n            return this.items[n].v;\n        },\n    setBorderWidth() {\n      let i = this.$refs.drawer.$el.querySelector(\n        \".v-navigation-drawer__border\"\n      );\n      i.style.width = this.navigation.borderSize + \"px\";\n      i.style.cursor = \"ew-resize\";\n      i.style.backgroundColor = \"green\";\n    },\n    setEvents() {\n      const minSize = this.navigation.borderSize;\n      const el = this.$refs.drawer.$el;\n      const drawerBorder = el.querySelector(\".v-navigation-drawer__border\");\n      const direction = el.classList.contains(\"v-navigation-drawer--right\")\n        ? \"right\"\n        : \"left\";\n\n      function resize(e) {\n        document.body.style.cursor = \"ew-resize\";\n        let f =\n          direction === \"right\"\n            ? document.body.scrollWidth - e.clientX\n            : e.clientX;\n        /*\n        if(f < 100)\n            f = 100;\n        if(f > 600)\n            f = 600;*/\n\n        el.style.width = f + \"px\";\n      }\n\n      drawerBorder.addEventListener(\n        \"mousedown\",\n        (e) => {\n          if (e.offsetX < minSize) {\n            el.style.transition = \"initial\";\n            document.addEventListener(\"mousemove\", resize, false);\n          }\n        },\n        false\n      );\n\n      document.addEventListener(\n        \"mouseup\",\n        () => {\n          el.style.transition = \"\";\n          this.navigation.width = el.style.width;\n          document.body.style.cursor = \"\";\n          document.removeEventListener(\"mousemove\", resize, false);\n        },\n        false\n      );\n    },\n      validateprocess(item)\n      {\n\n        if(item.active)\n        {\n            document.getElementById('validatebutton').style.color= \"darkgreen\";\n            this.sheet = false;\n        }\n        else\n        {\n            \n            document.getElementById('validatebutton').style.color= \"white\";\n            this.sheet = true;\n        }\n\n      }\n  },\n    watch: {\n      group () {\n        //this.drawer = false\n      },\n    },\n})\n</script>\n\n<style>\n    .bottomsheet{\n    position: absolute !important;\n    right: 0 !important;\n\n\n}\n\n</style>\n"],"sourceRoot":"src/components"}]}