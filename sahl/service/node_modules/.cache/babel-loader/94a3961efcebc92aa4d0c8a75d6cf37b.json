\n                    <v-list-item-title>{{ item.title }}</v-list-item-title>\r\n                </v-list-item>\r\n            </v-list>\r\n        </v-menu>\r\n        <v-menu offset-y>\r\n            <template v-slot:activator=\"{ on, attrs }\">\r\n                <v-btn text v-bind=\"attrs\" v-on=\"on\">\r\n                    Save\r\n                </v-btn>\r\n            </template>\r\n            <v-list>\r\n                <v-list-item v-for=\"(item, index) in saveItem\" :key=\"index\">\r\n                    <v-list-item-title>{{ item.title }}</v-list-item-title>\r\n                </v-list-item>\r\n            </v-list>\r\n        </v-menu>\r\n        <v-menu offset-y>\r\n            <template v-slot:activator=\"{ on, attrs }\">\r\n                <v-btn text v-bind=\"attrs\" v-on=\"on\">\r\n                    Tool\r\n                </v-btn>\r\n            </template>\r\n            <v-list>\r\n                <v-list-item v-for=\"(item, index) in toolItem\" :key=\"index\">\r\n                    <v-list-item-title>{{ item.title }}</v-list-item-title>\r\n                </v-list-item>\r\n            </v-list>\r\n        </v-menu>\r\n        <v-menu offset-y>\r\n            <template v-slot:activator=\"{ on, attrs }\">\r\n                <v-btn text v-bind=\"attrs\" v-on=\"on\">\r\n                    Search\r\n                </v-btn>\r\n            </template>\r\n            <v-list>\r\n                <v-list-item v-for=\"(item, index) in searchItem\" :key=\"index\">\r\n                    <v-list-item-title>{{ item.title }}</v-list-item-title>\r\n                </v-list-item>\r\n            </v-list>\r\n        </v-menu>\r\n        <v-menu offset-y>\r\n            <template v-slot:activator=\"{ on, attrs }\">\r\n                <v-btn text v-bind=\"attrs\" v-on=\"on\">\r\n                    Help\r\n                </v-btn>\r\n            </template>\r\n            <v-list>\r\n                <v-list-item v-for=\"(item, index) in helpItem\" :key=\"index\">\r\n                    <v-list-item-title>{{ item.title }}</v-list-item-title>\r\n                </v-list-item>\r\n            </v-list>\r\n        </v-menu>\r\n        <v-dialog v-model='dialogNewProject' width=\"500\" >\r\n            <v-card>\r\n                <v-card-title class=\"text-h6 green accent-1\"> Name New Project </v-card-title>\r\n                <v-card-text>\r\n                    <v-text-field single-line label=\"project name\" v-model=\"strProjectName\">\r\n                    </v-text-field>\r\n                </v-card-text>\r\n                <v-card-actions>\r\n                    <v-spacer></v-spacer>\r\n                    <v-btn class=\"d-inline-flex ml-3 mr-1\" color=\"green darken-1\" text  @click=\"addproject()\" >\r\n                        Accept\r\n                    </v-btn>\r\n                    <v-btn class=\"d-inline-flex ml-3 mr-1\" color=\"green darken-1\" text @click=\"cancelproject\">\r\n                        Cancel\r\n                    </v-btn>\r\n                </v-card-actions>\r\n            </v-card>\r\n        </v-dialog>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nexport default({\r\n    data() {\r\n        return {\r\n            fileItem: [\r\n                { title: 'New File'},\r\n                { title: 'Delete File'},\r\n                { title: 'Open'},\r\n                { title: 'Save'},\r\n                { title: 'Auto Save'},\r\n                { title: 'Revert File'},\r\n                { title: 'Close Screen'},\r\n            ],\r\n            projectItem: [\r\n                { title: 'New Prpject',    menuAction: action => { this.newProject(action) }},\r\n                { title: 'Delete Prpject'},\r\n                { title: 'Open'},\r\n                { title: 'Save'},\r\n                { title: 'Auto Save'},\r\n                { title: 'Revert Prpject'},\r\n                { title: 'Close Screen'},\r\n            ],\r\n            saveItem: [\r\n                { title: 'Save as'},\r\n            ],\r\n            toolItem: [\r\n                { title: 'Undo'},\r\n                { title: 'Redo'},\r\n                { title: 'Cut'},\r\n                { title: 'Copy'},\r\n                { title: 'Paste'},\r\n            ],\r\n            searchItem: [\r\n                { title: 'Replace'},\r\n            ],\r\n            helpItem: [\r\n                { title: 'Manual'},\r\n                { title: 'Search'},\r\n                { title: 'Setting'},\r\n            ],\r\n            dialogNewProject: false,\r\n            strProjectName: null,\r\n            \r\n        }\r\n    },\r\n    methods: {\r\n        newProject() {\r\n            this.dialogNewProject = true\r\n        },\r\n        addproject () {\r\n            this.$store.commit('addProject', {name:this.strProjectName})\r\n            this.$store.commit('selectOpenProject', {openProjectIndex: (this.$store.getters.projectCount -1 )})\r\n            this.$store.commit('setmakeProject', {makeproject:true})\r\n            this.cancelproject()\r\n        },\r\n        cancelproject () {\r\n            this.dialogNewProject = false\r\n            this.strProjectName = null\r\n        },\r\n\r\n    },\r\n})\r\n</script>\r\n"],"sourceRoot":"src/components"}]}